{
  "expo": {
    "name": "Sudoku Owl",
    "slug": "sudoku-owl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "sudokuowl",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5E6D3"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.giftowl.sudokuowl",
      "buildNumber": "1",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3527565745537185~1174233790"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5E6D3"
      },
      "package": "com.giftowl.sudokuowl",
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3527565745537185~1174233790"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-3527565745537185~1174233790",
          "androidAppId": "ca-app-pub-3527565745537185~1174233790"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2faf3936-f4b2-4bc8-a437-62ddf6f7beb2"
      }
    }
  }
}
